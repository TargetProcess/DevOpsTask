@{
    ViewBag.Title = "Accounts";
    Layout = "~/Views/shared/_BootstrapLayout.basic.cshtml";
}

@using Web.BootstrapSupport
@using System.Collections
@using System.Collections.Generic
@using Config.Model
@model IEnumerable<Account>
<h4>Accounts</h4>
<table class="table table-striped" id="accounts" cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered">
    <caption></caption>
    <thead>
        <tr>
            <th>Name</th>
        </tr>
    </thead>
    @foreach (var account in Model)
    {
        <tr>
            <td>
                @account.Name
            </td>
        </tr>
    }

</table>
